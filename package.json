{
  "name": "@autosoft/autorepo-api",
  "version": "0.2.1",
  "description": "API for Autorepo.",
  "main": ".auto/build/index.js",
  "author": "Connor White",
  "license": "MIT",
  "private": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/autosoftoss/autorepo-api.git"
  },
  "keywords": [
    "auto",
    "repo"
  ],
  "exports": "./.auto/build/index.js",
  "types": ".auto/build/index.d.ts",
  "files": [
    ".auto/build/**/!(tsconfig.tsbuildinfo)"
  ],
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "build": "swc source --out-dir .auto/build --copy-files && tsc --project ./tsconfig.json --emitDeclarationOnly --incremental",
    "dev": "ts-node-esm ./scripts/dev.ts",
    "lint": "eslint source --ext .ts",
    "prepack": "yarn build",
    "test": "jest"
  },
  "dependencies": {
    "@autosoft/eslint-config": "*",
    "@autosoft/jest-preset": "*",
    "@autosoft/tsconfig": "*",
    "@bconnorwhite/exec": "^6.0.12",
    "@octokit/rest": "^19.0.5",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.22",
    "clee": "^0.2.1",
    "conf": "^10.2.0",
    "conventional-changelog": "^3.1.25",
    "conventional-recommended-bump": "^6.1.0",
    "enquirer": "^2.3.6",
    "file-structure": "^1.6.2",
    "join-newlines": "^2.0.1",
    "mem": "^9.0.2",
    "npm-description": "^1.0.2",
    "npm-package-json-lint-config-auto": "*",
    "package-add": "^1.1.5",
    "package-run": "^2.0.6",
    "semver": "^7.3.8",
    "spdx-license": "^1.0.2",
    "tsd": "^0.25.0",
    "turbo": "^1.6.3",
    "types-pkg-json": "^1.2.1",
    "which-pm-lockfile": "^1.1.1"
  },
  "devDependencies": {
    "@autosoft/eslint-config": "*",
    "@autosoft/jest-preset": "*",
    "@autosoft/tsconfig": "*",
    "@types/conventional-changelog": "^3.1.1",
    "npm-package-json-lint-config-auto": "*"
  },
  "eslintConfig": {
    "extends": "@autosoft/eslint-config"
  },
  "jest": {
    "preset": "@autosoft/jest-preset"
  },
  "npmpackagejsonlint": {
    "extends": "npm-package-json-lint-config-auto"
  }
}
